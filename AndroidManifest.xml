<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds"
      android:versionCode="6"
      android:versionName="1.5"
      android:targetSdkVersion="13">
      
    <uses-sdk android:minSdkVersion="8" />
    
    
 
    
    <uses-permission android:name="android.permission.INTERNET"/> 
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <!--
    <supports-screens android:xlargeScreens="true" /> 
    -->
    

    
		 
 


    <application android:icon="@drawable/dollars" android:label="@string/app_name">
        <activity android:name=".StockCalculatorActivityAds"
                  android:label="@string/app_name" android:screenOrientation="portrait">>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
		<activity android:label="@string/results_page" android:name="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds.SCResultActivityAds" android:screenOrientation="portrait">
			- <intent-filter> 
		<action android:name="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds.SCRESULTACTIVITYADS" /> 
		<category android:name="android.intent.category.DEFAULT" /> 
		</intent-filter> 
		</activity> 
		
		<activity android:label="@string/error_page" android:name="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds.SCErrorActivityAds" android:screenOrientation="portrait">
			- <intent-filter> 
		<action android:name="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds.SCERRORACTIVITYADS" /> 
		<category android:name="android.intent.category.DEFAULT" /> 
		</intent-filter> 
		</activity> 
		
		<activity android:name=".StockViewActivity"
                  android:label="@string/stock_view_page" android:screenOrientation="portrait">>
            <intent-filter>
                <action android:name="net.dmpetersen.androiddevelopment.StockCalculatorQuotesAds.StockViewActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
		
		<activity android:name="com.google.ads.AdActivity"             
		     android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
		
		
		
		
    </application>
    
		
</manifest>